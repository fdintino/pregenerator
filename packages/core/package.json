{
  "name": "pregenerator",
  "version": "1.1.0-beta.1",
  "description": "JS source transforms for generators (and more) using a lightweight babel-types replacement",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "build": "rollup -c",
    "instrument": "cross-env NODE_ENV=test rollup -c",
    "watch": "rollup -c -w",
    "watch:instrument": "cross-env NODE_ENV=test rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdintino/pregenerator.git"
  },
  "author": "Frankie Dintino <fdintino@gmail.com> (https://www.frankiedintino.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fdintino/pregenerator/issues"
  },
  "homepage": "https://github.com/fdintino/pregenerator#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@pregenerator/ast-types": "^1.1.0-beta.1",
    "@pregenerator/babel-lite": "^1.1.0-beta.1",
    "@pregenerator/helpers": "^1.1.0-beta.1",
    "@pregenerator/transform": "^1.1.0-beta.1",
    "acorn": "^8.3.0",
    "astring": "^1.7.5",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@types/estree": "^0.0.48",
    "@types/lodash.clone": "^4.5.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^15.12.0"
  },
  "keywords": [
    "generator",
    "yield",
    "coroutine",
    "transformation",
    "syntax",
    "codegen",
    "rewriting",
    "refactoring",
    "transpiler",
    "desugaring",
    "regenerator",
    "pregenerator",
    "async",
    "6to5",
    "babel",
    "const",
    "es6",
    "harmony",
    "let",
    "transpile",
    "transpiler",
    "var",
    "compiler"
