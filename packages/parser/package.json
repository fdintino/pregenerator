{
  "name": "@pregenerator/parser",
  "version": "1.1.2",
  "description": "parser wrapping acorn, used by pregenerator",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "jsnext:main": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "browser": "dist/cjs/index.min.js",
  "files": ["src", "dist"],
  "exports": {
    "node": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "script": "./dist/cjs/index.min.js",
    "import": "./dist/mjs/index.min.js",
    "default": "./dist/cjs/index.js",
    "types": "./dist/mjs/index.d.ts"
  },
  "scripts": {
    "build": "rollup -c",
    "instrument": "cross-env NODE_ENV=test rollup -c",
    "watch": "rollup -c -w",
    "watch:instrument": "cross-env NODE_ENV=test rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdintino/pregenerator.git"
  },
  "author": "Frankie Dintino <fdintino@gmail.com> (https://www.frankiedintino.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fdintino/pregenerator/issues"
  },
  "homepage": "https://github.com/fdintino/pregenerator#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@pregenerator/ast-types": "^1.1.1",
    "acorn": "^8.3.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.0"
  },
  "keywords": [
    "generator",
    "yield",
    "coroutine",
    "transformation",
    "syntax",
    "codegen",
    "rewriting",
    "refactoring",
    "transpiler",
    "desugaring",
    "regenerator",
    "pregenerator",
    "async",
    "6to5",
    "babel",
    "const",
    "es6",
    "harmony",
    "let",
    "transpile",
    "transpiler",
    "var",
    "compiler"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "18c9b449142d15a84415b36a593dc8b605e81e33"
}
